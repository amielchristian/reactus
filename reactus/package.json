{
  "type": "module",
  "name": "reactus",
  "version": "0.1.18",
  "license": "Apache-2.0",
  "description": "reactive React template engine",
  "author": "Chris <chris@incept.asia>",
  "homepage": "https://github.com/stackpress/reactus",
  "bugs": "https://github.com/stackpress/reactus/issues",
  "repository": "stackpress/reactus",
  "keywords": [
    "ts",
    "typescript",
    "template engine",
    "react",
    "vite",
    "stackpress"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./tsconfig/client": "./tsconfig/client.json",
    "./assets.d.ts": "./tsconfig/assets.d.ts",
    "./constants": "./dist/constants.js",
    "./helpers": "./dist/helpers.js",
    "./types": "./dist/types.js",
    "./Builder": "./dist/Builder.js",
    "./Document": "./dist/Document.js",
    "./DocumentBuilder": "./dist/DocumentBuilder.js",
    "./DocumentLoader": "./dist/DocumentLoader.js",
    "./DocumentRender": "./dist/DocumentRender.js",
    "./Exception": "./dist/Exception.js",
    "./Manifest": "./dist/Manifest.js",
    "./Server": "./dist/Server.js",
    "./ServerLoader": "./dist/ServerLoader.js",
    "./ServerManifest": "./dist/ServerManifest.js",
    "./ServerResource": "./dist/ServerResource.js",
    "./VirtualServer": "./dist/VirtualServer.js",
    "./fouc.css": "./fouc.css"
  },
  "typesVersions": {
    "*": {
      "index": [ "./dist/index.d.ts"],
      "constants": [ "./dist/constants.d.ts" ],
      "helpers": [ "./dist/helpers.d.ts" ],
      "types": [ "./dist/types.d.ts" ],
      "Builder": [ "./dist/Builder.d.ts" ],
      "Document": [ "./dist/builder/Document.d.ts" ],
      "DocumentBuilder": [ "./dist/DocumentBuilder.d.ts" ],
      "DocumentLoader": [ "./dist/DocumentLoader.d.ts" ],
      "DocumentRender": [ "./dist/DocumentRender.d.ts" ],
      "Exception": [ "./dist/Exception.d.ts" ],
      "Manifest": [ "./dist/Manifest.d.ts" ],
      "Server": [ "./dist/Server.d.ts" ],
      "ServerLoader": [ "./dist/ServerLoader.d.ts" ],
      "ServerManifest": [ "./dist/ServerManifest.d.ts" ],
      "ServerResource": [ "./dist/ServerResource.d.ts" ],
      "VirtualServer": [ "./dist/VirtualServer.d.ts" ]
    }
  },
  "files": [
    "dist",
    "fouc.css",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "nyc ts-mocha -r tsx tests/*.test.ts",
    "report": "nyc report -r lcov"
  },
  "dependencies": {
    "@stackpress/lib": "0.5.8",
    "@vitejs/plugin-react": "4.3.4",
    "vite": "6.1.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.20",
    "@types/mocha": "10.0.10",
    "@types/node": "22.9.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "chai": "4.5.0",
    "mocha": "10.8.2",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "17.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "typescript": "5.7.2"
  }
}